{
  "name": "Anti Kappa",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Filters spam from twitch.tv",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "src/inject.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.twitch.tv/*"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ]
}
